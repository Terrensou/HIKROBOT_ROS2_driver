{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Camera Driver",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/hkcamera_driver/hkcamera_driver",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
